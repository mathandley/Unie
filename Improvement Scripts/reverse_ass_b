
----------------------------------------------------------------------------------------------------
-- Commands: --
----------------------------------------------------------------------------------------------------
import-lib ./libs/reverse
import-lib EVAL ./libs/reverse_eval_cec1
trans $
      V_Abs[V_Rep[V_Revbody[f]]]
      $ WCE $
        V_Revbody[f]
        $
apply-def 'Abs
apply-def 'Rep
right
left
beta
up
beta
top
apply-def 'Revbody
right
left
right
beta
up
up
case-eval
right
rhs
eval-wce
up
next
rhs
append-assoc-lr-wce
right
eval-wce
top
unapply-def 'Revbody
exit-script


