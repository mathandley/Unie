let 
   (++) = λxs.λys.case xs of
           []      ➞ ys
           (z::zs) ➞ let rs = zs ++ ys
                     in (z::rs)
 
   reverse = let f = λxs.λys.case xs of 
                      []      ➞ ys
                      (z::zs) ➞ let a = (z::ys) in f zs a
             in λxs.let a = [] in f xs a
        
in reverse [1..10]