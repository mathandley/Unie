    { trans $ V_Abs[V_Rep[V_F[y]]] $ WCE $ V_F[y] $ }

V_Abs[V_Rep[V_F[y]]]

  ≡ { right }

V_Abs[V_Rep[V_F[y]]]

  ≡ { right }

V_Abs[V_Rep[V_F[y]]]

 ⪅⪆ { tick-intro }

V_Abs[V_Rep[✔V_F[y]]]

  ≡ { top }

V_Abs[V_Rep[✔V_F[y]]]

 ≲≳ { ungc $ x = V_F[y] $ }

let x = V_F[y] in V_Abs[V_Rep[✔V_F[y]]]

 ≲≳ { unvalue-beta 'x $ let x = V_F[y] in V_Abs[V_Rep[-]] $ }

let x = V_F[y] in V_Abs[V_Rep[x]]

  ≡ { right }

let x = V_F[y] in V_Abs[V_Rep[x]]

 ⪅⪆ { ww-ass-a $ V_Abs[-] $ $ V_Rep[-] $ }

let x = V_F[y] in x

  ≡ { top }

let x = V_F[y] in x

 ≲≳ { value-beta 'x $ let x = V_F[y] in [-] $ }

let x = V_F[y] in ✔V_F[y]

  ≡ { right }

let x = V_F[y] in ✔V_F[y]

 ⪅⪆ { untick-intro }

let x = V_F[y] in V_F[y]

  ≡ { top }

let x = V_F[y] in V_F[y]

 ≲≳ { gc 'x }

V_F[y]



