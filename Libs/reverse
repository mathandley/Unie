
----------------------------------------------------------------------------------------------------
-- Terms: -----
----------------------------------------------------------------------------------------------------
(++) = λxs.λys.case xs of
                []       ➞ ys
                (z : zs) ➞ let rs = zs ++ ys in (z : rs)
 
reverse = let f = V_Revbody[f] in f

----------------------------------------------------------------------------------------------------
-- Contexts: --
----------------------------------------------------------------------------------------------------
V_Abs = λxs.[-] xs []
 
V_Rep = λxs.λys.([-] xs) ++ ys
 
V_Revbody = λxs.case xs of
                 []       ➞ []
                 (y : ys) ➞ ([-] ys) ++ [y]


