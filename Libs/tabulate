V_Abs = λf.[-] 0 f
 
V_F = λf.let g = λx.let y = x + 1 in f y
             a = f 0
             b = [-] g
         in (a::b)
 
V_Rep = λn.λf.let g = λx.let y = x + n in f y in [-] g


